extends layout

block content
    .row.banner
        .col-12
            h1= "default"

    .row
        .col-12.col-md-8
            .quizsets
                each set in sets
                    .card
                        .card-block
                            h4
                                a(href=`/sets/${set._id}`)= set.title
                            p= set.subTitle
